{
  "name": "patient-platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "npm run dev:h5",
    "dev:h5": "cross-env VUE_APP_ENV=development NODE_ENV=development UNI_PLATFORM=h5 vue-cli-service uni-serve",
    "dev:mp-alipay": "cross-env NODE_ENV=development UNI_PLATFORM=mp-alipay vue-cli-service uni-build --watch",
    "dev:mp-weixin": "cross-env VUE_APP_ENV=development NODE_ENV=development UNI_PLATFORM=mp-weixin vue-cli-service uni-build --watch",
    "test:h5": "cross-env NODE_ENV=test UNI_PLATFORM=h5 vue-cli-service uni-build",
    "test:h5-watch": "cross-env NODE_ENV=test UNI_PLATFORM=h5 vue-cli-service uni-serve",
    "test:mp-alipay": "cross-env NODE_ENV=test UNI_PLATFORM=mp-alipay vue-cli-service uni-build --watch",
    "test:mp-weixin": "cross-env NODE_ENV=test UNI_PLATFORM=mp-weixin vue-cli-service uni-build --watch",
    "pre:h5": "cross-env NODE_ENV=pre UNI_PLATFORM=h5 vue-cli-service uni-build",
    "pre:h5-watch": "cross-env NODE_ENV=pre UNI_PLATFORM=h5 vue-cli-service uni-serve",
    "pre:mp-alipay": "cross-env NODE_ENV=pre UNI_PLATFORM=mp-alipay vue-cli-service uni-build --watch",
    "pre:mp-weixin": "cross-env VUE_APP_ENV=pre NODE_ENV=pre UNI_PLATFORM=mp-weixin UNI_OUTPUT_DIR=dist/pre/mp-weixin vue-cli-service uni-build --watch --minimize",
    "build": "npm run build:h5",
    "build:h5-dev": "cross-env VUE_APP_ENV=development  NODE_ENV=development UNI_PLATFORM=h5 vue-cli-service uni-build",
    "build:h5-prod": "cross-env VUE_APP_ENV=production  NODE_ENV=production UNI_PLATFORM=h5 UNI_OUTPUT_DIR=dist/prod/h5 vue-cli-service uni-build",
    "build:h5": "cross-env VUE_APP_ENV=production VUE_APP_ENV=production NODE_ENV=production UNI_PLATFORM=h5 vue-cli-service uni-build",
    "build:mp-alipay": "cross-env NODE_ENV=production UNI_PLATFORM=mp-alipay vue-cli-service uni-build",
    "build:mp-weixin": "cross-env VUE_APP_ENV=production NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service uni-build --minimize",
    "build:mp-weixin-pre": "cross-env VUE_APP_ENV=pre NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service uni-build --minimize",
    "build:mp-weixin-watch": "cross-env NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service uni-build --minimize --watch",
    "info": "node node_modules/@dcloudio/vue-cli-plugin-uni/commands/info.js",
    "report": "vue-cli-service build --report",
    "jest:h5": "cross-env UNI_PLATFORM=h5 jest -i",
    "jest:mp-weixin": "cross-env UNI_PLATFORM=mp-weixin jest -i"
  },
